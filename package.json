{
  "name": "gainz-template-1",
  "version": "1.0.0",
  "description": "A Gainz Template Project - replace me",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build --webpack",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --webpack",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev --webpack",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:sw": "node scripts/generate-sw.js",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "setup": "node scripts/setup.js",
    "setup:colors": "node scripts/setup.js --colors",
    "setup:colors:manual": "node scripts/setup.js --manual-colors",
    "setup:images": "node scripts/setup.js --images",
    "setup:full": "node scripts/setup.js --full",
    "setup:claude": "node scripts/setup.js --claude",
    "test-sheets": "node scripts/test-sheets.js",
    "generate:images": "node scripts/generate-images.js",
    "ai:image": "node scripts/generate-images.js",
    "ai:list": "node scripts/list-generated-images.js",
    "ai:examples": "node scripts/logo-examples.js",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "db-create": "gainz create --db-only"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^5.13.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@payloadcms/db-postgres": "3.59.1",
    "@payloadcms/email-resend": "^3.59.1",
    "@payloadcms/next": "3.59.1",
    "@payloadcms/payload-cloud": "3.59.1",
    "@payloadcms/richtext-lexical": "3.59.1",
    "@payloadcms/ui": "3.59.1",
    "@tailwindcss/postcss": "^4.1.14",
    "cross-env": "^7.0.3",
    "culori": "^4.0.2",
    "dotenv": "16.4.7",
    "google-auth-library": "^10.5.0",
    "googleapis": "^166.0.0",
    "graphql": "^16.8.1",
    "next": "^16.0.0",
    "payload": "3.59.1",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.34.2",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@playwright/test": "1.54.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "prettier": "^3.4.2",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
